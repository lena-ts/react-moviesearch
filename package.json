{
  "name": "react-moviesearch",
  "homepage": "https://lena-ts.github.io/react-moviesearch",
  "version": "1.0.0",
  "description": "React app",
  "main": "index.js",
  "scripts": {
    "client": "webpack --config src/client/webpack.config.js --mode development",
    "server": "webpack --config src/server/webpack.config.js --mode development",
    "client:watch": "yarn client --watch",
    "server:watch": "yarn server --watch",
    "server:run": "nodemon dist/server.js",
    "server:open": "open http://localhost:8080/react-moviesearch",
    "server:start": "npm-run-all --parallel client:watch server:watch server:run server:open",
    "dev": "cross-env NODE_ENV=development webpack --config src/client/webpack.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config src/client/webpack.config.js --mode production",
    "deploy": "gh-pages -d static",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "pretty": "pretty-quick",
    "cy:run": "cypress open",
    "e2e": "start-server-and-test start http://localhost:8080 cy:run",
    "test": "jest -u",
    "test:cover": "jest --coverage",
    "open:coverage": "open ./coverage/lcov-report/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn run lint && yarn test"
    }
  },
  "standard": {
    "env": [
      "cypress/globals"
    ]
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "author": "Lena Tsybulenko",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "next": "^11.1.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.9.6",
    "@babel/runtime-corejs3": "^7.9.6",
    "autoprefixer": "^9.7.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^7.1.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "cross-fetch": "^3.0.4",
    "css-loader": "^3.4.2",
    "cypress": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "favicons-webpack-plugin": "^3.0.1",
    "file-loader": "^5.1.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nock": "^12.0.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.1",
    "start-server-and-test": "^1.10.11",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
